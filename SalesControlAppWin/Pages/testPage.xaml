<Page x:Class="PresentationLayer.Pages.testPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PresentationLayer.Pages"
xmlns:ssoft='clr-namespace:SaeediSoftWpfUiControls;assembly=SaeediSoftWpfUiControls'
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="testPage">

    <Grid>
        <ssoft:ComboBoxAutoComplete Loaded="uc_cbox_Loaded"
                            FilterMode="Contains"
                            HorizontalAlignment="Stretch"
                            VerticalAlignment="Center"
                            x:Name="uc_cbox"
                            DisplayMemberPath="DisplayField" />
    </Grid>
</Page>
